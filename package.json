{
  "name": "cerita-di-sekitarmu",
  "version": "1.0.0",
  "description": "Aplikasi berbagi cerita berbasis lokasi",
  "homepage": "https://gussakah00.github.io/po1",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "start-dev": "cross-env NODE_ENV=development webpack serve --config webpack.dev.js --host localhost",
    "serve": "http-server dist -p 8080 -c-1",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
